	<script>
		ui.loader('index', function() {
			var vm = new Vue({
				el: '#webApp',
				data: $.extend(vue.data, {
					marketAnalysis: {
						imageUrl: '',
						list: []
					},
					coreBusiness: [],
					latestNews: [],
					links: []
				}),
				methods: $.extend(vue.methods, {
					jumpUrl: function(nav, url) {
						var $nav = $('li[data-nav="' + nav + '"]', '#headerNav');
						if(!url)
							window.location.href = $('a', $nav).attr('href');
						else
							window.location.href = url + '&menuid=' + $nav.attr('id');
					}
				}),
				ready: function() {
					$('a', '#newsNotice').click(function() {
						window.location.href = $('li[data-nav="/Site/web/advantage.html"]', '#headerMenu').find('a').attr('href');
					});
					this.$http.get(this.interfaceUrl + '/advantage/interface/list?page=1&limit=3').then(function(response) {
						//							this.marketAnalysis.imageUrl = this.siteUrl+'/src/images/index/market-analysis.jpg';
						this.marketAnalysis.list = response.body.page.list;
					});
					this.$http.get(this.interfaceUrl + '/business/interface/list?page=1&limit=6').then(function(response) {
						this.coreBusiness = response.body.page.list;
						this.$nextTick(function() {
							$('.t_1200_3 li').hover(function() {
								$(this).stop().animate({
									margin: "0 31px 14px 0"
								});

							}, function() {
								$(this).stop().animate({
									margin: "14px 31px 0 0"
								});
							})
						})
					});
					this.$http.get(this.interfaceUrl + '/news/interface/list?page=1&limit=1').then(function(response) {
						this.latestNews = response.body.page.list;
					});
					this.$http.get(this.interfaceUrl + '/links/interface/list?page=1&limit=6').then(function(response) {
						this.links = response.body.page.list;
					});
				}
			});
		});
	</script>


