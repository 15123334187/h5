	<script>
		ui.loader('index', function() {
			var vm = new Vue({
				el: '#webApp',
				data: $.extend(vue.data, {
					marketAnalysis: {
						imageUrl: '',
						list: []
					},
					coreBusiness: [],
					latestNews: [],
					links: []
				}),
				methods: $.extend(vue.methods, {
					jumpUrl: function(nav, url) {
						var $nav = $('a[data-nav="' + nav + '"]', '#headerNav');
						if(!url)
							window.location.href = $nav.attr('href');
						else
							window.location.href = url + '&menuid=' + $nav.attr('id');
					}
				}),
				ready: function() {
					this.$http.get(this.interfaceUrl + '/advantage/interface/list?page=1&limit=8').then(function(response) {
						//							this.marketAnalysis.imageUrl = this.siteUrl+'/src/images/index/market-analysis.jpg';
						this.marketAnalysis.list = response.body.page.list;
					});
					this.$http.get(this.interfaceUrl + '/business/interface/list?page=1&limit=5').then(function(response) {
						this.coreBusiness = response.body.page.list;
						this.$nextTick(function() {
							$('.jQ_sliderSwitch li').on('click', function() {
								var $elem = $(this);
								var index = $elem.index();
								$('#img-slider').roundabout('animateToChild', index);
								return false;
							}).first().addClass('active');
							$('#img-slider').roundabout({
								minScale: 0.35,
								maxScale: 0.4,
								duration: 750
							}).bind({
								animationEnd: function(e) {
									var index = $('#img-slider').roundabout('getChildInFocus');
									$('.jQ_sliderSwitch li').removeClass('active');
									$('.jQ_sliderSwitch li').eq(index).addClass('active');
								}
							});
						})
					});
					this.$http.get(this.interfaceUrl + '/news/interface/list?page=1&limit=8').then(function(response) {
						this.latestNews = response.body.page.list;
					});
					this.$http.get(this.interfaceUrl + '/links/interface/list?page=1&limit=6').then(function(response) {
						this.links = response.body.page.list;
					});
				}
			});
		});
	</script>

